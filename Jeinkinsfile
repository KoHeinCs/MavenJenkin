pipeline {
    agent any
    stages {
        stage('Init'){
            steps {
                echo 'Hi , this is KoHeinCs from Myanmar'
                echo 'We are starting the testing'
            }
        }
        stage('Build'){
            steps {
                echo 'Building Sample Maven Project'
            }
        }
        stage('Deploy'){
            steps {
                echo 'Deploying in Staging Area'
            }
        }
    }
}